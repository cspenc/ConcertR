<%= form_for @user do |f| %>

  <%= f.label :first %>
  <%= f.text_field :first %>

  <%= f.label :last %>
  <%= f.text_field :last %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :bio %>
  <%= f.text_area :bio %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_conf %>
  <%= f.password_field :password_conf %>

  <%= f.submit "Create Your Account" %>

<% end %>
